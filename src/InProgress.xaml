<Window x:Class="Cutting_Optimizer.InProgress"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cutting_Optimizer"
        mc:Ignorable="d"
        Title="InProgress" Height="150" Width="200" 
        MinHeight="150" MaxHeight="150"
        MinWidth="200" MaxWidth="200"
        Background="#FF323232"
        WindowStartupLocation="CenterScreen">
    <StackPanel x:Name="sp">
        <TextBlock Text="In Progress" HorizontalAlignment="Center" Margin="0, 23, 0, 8"  Foreground="#FFEAEAEE" FontSize="20"/>
        <TextBlock Background="#FF616266" Height="3" Foreground="#FFEAEAEE"/>
        <TextBlock Text="Please wait." HorizontalAlignment="Center" Margin="8" Foreground="#FFEAEAEE" FontSize="20"/>
    </StackPanel>
</Window>
